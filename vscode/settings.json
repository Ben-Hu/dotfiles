{
  "breadcrumbs.enabled": true,
  "editor.acceptSuggestionOnEnter":"on",
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.largeFileOptimizations": false,
  "editor.tabSize": 2,
  "editor.rulers": [100],
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "window.zoomLevel": 0,
  "window.menuBarVisibility": "compact",
  "window.titleBarStyle": "custom",
  "workbench.editor.showTabs": true,
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#424242",
    "titleBar.inactiveBackground": "#424242",
    "titleBar.activeForeground": "#bdbdbd",
    "titleBar.inactiveForeground": "#bdbdbd",
    "statusBar.foreground": "#bdbdbd",
    "statusBar.background" : "#bdbdbd",
    "statusBar.noFolderBackground" : "#424242",
    "statusBar.debuggingBackground": "#424242"
  },
  "zenMode.hideTabs": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.centerLayout": false,
  "zenMode.fullScreen": false,
  "python.formatting.provider": "black",
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "python.sortImports.args":["-rc", "-sp isort.cfg"],
  "[python]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave":{
      "source.organizeImports":true
    },
    "editor.tabSize": 4
  },
  "[elixir]": {
    "editor.formatOnSave": true
  },
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "[go]":  {
    "editor.formatOnSave": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.format.enable": true,
  "javascript.autoClosingTags": true,
  "javascript.format.semicolons": "insert",
  "javascript.preferences.quoteStyle": "double",
  "[javascript]": {
    "editor.formatOnSave": true
  }
}
